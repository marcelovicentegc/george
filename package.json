{
  "name": "home-automation-model",
  "version": "1.0.0",
  "main": "index.tsx",
  "author": "Marcelo Cardoso",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js && tsc --p tsconfig.server.json ",
    "client": "webpack-dev-server --progress",
    "client:wait": "wait-on tcp:4000 && yarn run client",
    "types": "gql-gen --config codegen.yml",
    "server": "nodemon",
    "e2e": "cypress run --spec \"e2e/integration/home.js\"",
    "e2e:wait": "wait-on tcp:4000 && cypress run --spec \"e2e/integration/home.js\"",
    "test-client": "jest \"(/__tests__/.*|(\\.|/)(client))\\.tsx?$\" --coverage",
    "test-client:watch": "yarn test-client --watch",
    "test-server": "jest \"(/__tests__/.*|(\\.|/)(server))\\.tsx?$\" --coverage",
    "test-server:wait": "wait-on tcp:4000 && yarn test-server",
    "test-server:alone": "concurrently --kill-others --success first \"yarn run server\" \"yarn run test-server:wait\"",
    "start": "cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client:wait\"",
    "start-with-e2e": "cross-env concurrently NODE_ENV=development \"yarn run server\" \"yarn run client:wait\" \"yarn run e2e:wait\" ",
    "upgd": "yarn upgrade-interactive",
    "launch": "cross-env NODE_ENV=production node ./dist/server/index.js"
  },
